<%# frozen_string_literal: true %>
<% if not(user.guest?) %>
  <% if user.concurrent_session_courante? %>
    <%= panneau_icarien_concurrent_session_courante %>
  <% elsif user.concurrent? %>
    <%= panneau_icarien_ancien_concurrent %>
  <% else %>
    <%= panneau_signup_pour_icarien %>
  <% end %>
<% elsif concurrent && not(concurrent.current?) %>
  <%= panneau_ancien_concurrent %>
<% else %>
  <%= panneau_visiteur_quelconque %>
<% end %>
