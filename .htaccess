AddHandler cgi-script .rb
Options +ExecCGI
Options -Indexes

DirectoryIndex index.rb

Options +FollowSymlinks
RewriteEngine on

RewriteCond %{REQUEST_FILENAME} !.+\.(css|js|png|jpg|ico)$

#RewriteCond %{REMOTE_ADDR} ::1
#RewriteCond %{REMOTE_ADDR} 2a01:cb1c:98:2e00:890:fc7d:2811:3506
#RewriteRule ^(.*)$ index2020.rb?route_init=$1 [QSA,L]

RewriteRule ^(.*)$ index.rb?route_init=$1 [QSA,L]
